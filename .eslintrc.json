{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "linebreak-style": 0,
        "no-console": 0,
        "indent": ["error", 4],
        "no-underscore-dangle": ["error", {
            "allow": ["_id"]
        }]
    }
}
